#geolocate {
  max-width: 400px;
  border: 1px solid transparent;
  box-sizing: border-box;
  height: 100vh;
}

.altsign {
  font-style: italic;
  color: #808080;
  margin-top: 5px;
  text-align: center;
}

.address-container {
  height: 35px;
  width: 100%;
  margin: 0;
  display: table;
  padding: 10px;
  background-color: rgba(255,255,255,0.2);
  border: 1px solid #f1f1f1;
}

.address-container > span {
  font-size: 18px;
  display: table-cell;
  text-align: right;
  vertical-align: middle;
}

#address {
  font-size: 14px;
  width: 94%;
  font-weight: 400;
  color: #4d4d4d;
  white-space: normal;
  margin-left: 10px;
  display: table-cell;
  text-align: left;
  vertical-align: middle;
}

.sectionline {
  font-size: 16px;
  font-weight: 100;
  color: #808080;
  margin-left: 36px;
  margin-top: 0;
  padding: 0;
}

#search-zip input {
  border: 1px solid #c6c6c6;
  width: 100%;
  height: 50px;
  font-size: 16px;
  font-weight: 100;
  padding: 5px 10px;
}

#search-zip p {
  font-size: 16px;
  font-weight: 100;
  text-align: left;
}

.getstats a {
  display: block;
  width: 100%;
  height: 50px;
  padding: 3.5% 0%;
  margin-top: 20px;
  text-transform: uppercase;
  font-size: 14px;
  border: 1px solid #29A5DC;
  color: #29A5DC;
  font-weight: 800;
  background-color: #fff;
  vertical-align: middle;
  cursor: pointer;
  text-align: center;
}

.getpark {
  display: block;
  width: 100%;
  height: 45px;
  margin: 0 auto;
  border-radius: 0px;
  padding: 1.5% 0%;
  text-transform: uppercase;
  font-size: 12px;
  border: 1px solid transparent;
  background-color: #f25d59;
  color: #fff;
  font-weight: 800;
  vertical-align: middle;
  cursor: pointer;
  text-align: center;
}

ul li {
  display: block;
}

input, button {
  outline: 0;
}

