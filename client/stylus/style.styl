  font-size:  14px !important;
  border-top: 1px solid #ccc;
  padding-top: 10px;
}







body {
  font-family: "Roboto", Verdana, sans-serif;
  -webkit-font-smoothing: antialiased;
}

.guide {
  text-align: center;
  color: #fff;
  clear: both;
  overflow: hidden;
  position: absolute;
  top:0;
  bottom:0;
}

.guide.blue {
  background-color: #01afdb;
}

.guide.green {
  background-color: #61af23;
}

.guide.orange {
  background-color: #fea427;
}

.guide.purple {
  background-color: #8d58d3;
}

.guide.red {
  background-color: #f24965;
}

.guide-control.blue {
  border-top: 1px solid #47c8e5;
}

.guide-control.green {
  border-top: 1px solid #80bd4c;
}

.guide-control.orange {
  border-top: 1px solid #feb959;
}

.guide-control.purple {
  border-top: 1px solid #a67cdc;
}

.guide-control.red {
  border-top: 1px solid #f47d97;
}

.guide-content {
  padding: 40px;
}

.guide-description {
  text-align: left;
  margin-top: 50px;
  line-height: 18px;
}

.guide-description h3 {
  font-size: 22px;
  margin-bottom: 10px;
  font-weight: 400;
}

.guide-control {
  text-align: center;
  height: 50px;
  width: 100%;
  max-width: 400px;
  position: fixed;
  bottom:0;
}

.guide-control ul {
  padding: 0;
}



.guide-control ul li {
  display: inline-block;
  text-align: center;
}

.guide-control ul li a {
  color: #fff;
  display: block;
  text-decoration: none;
}

.guide-control ul li:first-child a {
  padding: 15px 50px 10px 50px;
}

.guide-control ul li:last-child a {
  padding: 15px 50px 10px 50px;
}

.guide-control ul li > span {
  color: #fff;
  font-size: 16px;
  padding: 15px 0px 10px 0px;
}

.guide-control ul li:first-child {
  float: left;
}

.guide-control ul li:last-child {
  float: right;
}



.add-profile-photo {
  width: 80px;
  height: 80px;
  border-radius: 80px;
  margin: 0 auto;
  text-align: center;
  border: 1px solid #4a4d59;
  font-size: 14px;
  vertical-align: middle;
  color: #29abe2;
}

.add-profile-photo p {
  margin: 5px 0px 0px 0px;
  padding: 0;
  line-height: 19px;
}

.add-profile-photo span {
  font-size: 28px;
  font-weight: 100;
  padding: 0;
  vertical-align: top;
  margin: 0 auto;
  display: block;
  width: 30px;
  height: 25px;
}






#container {
  margin: 0 auto;
  max-width: 400px;
}

.main {
  /*margin: 60px 0px 80px 0px;*/
  background-color: #f1f1f1;
  width: 100%;
}

.main-nav {
  width: 100%;
  margin:0;
  padding: 0;
}

.main-nav li a {
  padding: 15px 20px;
  display: block;
  border-left: 4px solid transparent;
  color: #a7a9b1;
  font-size: 15px;
}


.main-nav li a:hover {
  border-left: 4px solid #29abe2;
  background-color: #20232e;
  color: #a7a9b1;
  outline: none;

}
  .darkblue {
    background-color: #2b2f3d;
  }

    .main-nav-photo {
      width: 35px;
      height: 35px;
      border-radius: 35px;
      display:inline-block;
      vertical-align: middle;
      margin-right: 10px;
    }

        .main-nav-name {
          font-size: 16px;
          color: #a7a9b1;
          display: inline-block;
          vertical-align: middle;
          text-transform: capitalize;
        }

.main-nav-name span {
  display: block;
  color: #636776;
  font-size: 11px;
}
  .sidenav-header {
    background-color: #20232e;
    padding: 20px 20px;
  }

    .header {
      width: 100%;
      display: block;
      padding: 10px 0px 0px 0px;
      margin: 0px 0px 0px 0px;
      clear: both;
      background-color: #f25d59;
    }

.header h2 {
  font-size:18px;
}

.header-items {
  margin:0px 0px 0px 0px;
  padding: 0px 0px 0px 20px;
}

  .header-items li {
    display: inline-block;
    color: #fff;
    padding:0;
    font-size: 18px;
    vertical-align: middle;
    margin-right: 5px;
  }

.main-menu-toggle {
  background-color: transparent;
  display: inline-block;
  text-align: center;
  vertical-align: top;
  border: 1px solid transparent;
  padding: 5px 10px;
}

.main-menu-toggle:hover {
  border: 1px solid #ccc;
  /*background-color: #f1f1f1;*/
}


