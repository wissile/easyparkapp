#carinfo {
  width: 80%;
  padding: 0;
  font-size: 38px;
  font-weight: 100;
  color: #808080;
  margin: 0 auto
}

#carinfo li {
  display: inline-block;
  text-align: center;
  padding: 10px;
  margin: 0;
  vertical-align: top;
}

#carinfo li p {
  padding: 0;
  margin: 0;
  font-size: 12px;
  font-weight: 400;
  text-align: center;
}

.home-search {
  border: 1px solid #f1f1f1;
  display: table;
  width: 100%;
  padding: 0px 10px 0px 0px;
}

.home-search span {
  font-size: 18px;
  display: table-cell;
  text-align: right;
  vertical-align: middle;
}

#Autocomplete {
  width: 94%;
  height: 40px;
  margin:0;
  display: table-cell;
  border: none;
  text-align: left;
  padding: 5px 15px;
  box-sizing: border-box;
  font-size: 14px;
  font-weight: 400;
  font-family: "Roboto";
  color: #4d4d4d;
}


#registerlist {
  padding: 0;
  margin: 10px 10px;
}

.name {
  float: left;
  width: 50%;
}

#profile-intro {
  font-size: 28px;
  font-weight: 100;
  color: #808080;
  text-align: center;
  margin-top: 20px;
}

#profile-intro p {
  font-size: 12px;
  font-weight: 400;
  margin: 0;
  padding: 0;
}

#registerlist li {
  margin: 10px 0px;
}

.register {
  width: 100%;
  height: 50px;
  border: 0;
  border-bottom: 1px solid #f1f1f1;
  padding: 5px 15px;
  box-sizing: border-box;
  font-size: 22px;
  font-weight: 100;
  font-family: "Roboto";
  color: #4d4d4d;
}

.register:focus {
  border-bottom: 1px solid #29A5DC;
}

.list-group {
  z-index: 1;
}

.list-group-item {
  border-left: 4px solid #ffff00;
}

.parking-name {
  font-size: 20px;
  font-weight: 400;
  color: #808080;
  margin-bottom: 10px;
}

.right-data {
  position: absolute;
  right: 0;
  top: 0;
  margin: 6px 6px 0px 0px;
}

.distance {
  display: inline-block;
  font-size: 32px;
  font-weight: 100;
}

.distance span {
  font-size: 18px;
}

.price {
  display: inline-block;
  font-size: 32px;
  margin-right: 5px;
  font-weight: 100;
}

.price span {
  font-size: 18px;
  vertical-align: top;
}

.type {
  color: #808080;
  display: inline-block;
}

.status {
  color: #808080;
  display: inline-block;
  margin-left: 10px;
  padding: 0px 5px;
  font-size: 14px;
  border-radius: 10px;
  background-color: #f3761c;
  color: #fff;
}


